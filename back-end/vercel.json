{
  "version": 2,
  "builds": [
    {
      "src": "./src/app.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
